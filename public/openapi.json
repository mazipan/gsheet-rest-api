{
  "openapi": "3.0.4",
  "info": {
    "title": "GSheet Rest API",
    "description": "[Home](/) • [Example](/example) • [Open API](/openapi.json) • [Postman](/gsheet-rest-api.postman_collection.json) \n\nEffortless REST API for your Google Sheet. Instantly turn your Google Sheet into a powerful API. \n\n [![GitHub Repo stars](https://img.shields.io/github/stars/mazipan/gsheet-rest-api)](https://github.com/mazipan/gsheet-rest-api) ![GitHub forks](https://img.shields.io/github/forks/mazipan/gsheet-rest-api) \n\n [![Deploy with Vercel](https://vercel.com/button)](https://vercel.com/new/clone?repository-url=https%3A%2F%2Fgithub.com%2Fmazipan%2Fgsheet-rest-api)",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:3000/api/v1"
    }
  ],
  "tags": [
    {
      "name": "Sheet",
      "description": "Managing spreadsheet"
    }
  ],
  "paths": {
    "/sheets": {
      "get": {
        "tags": ["Sheet"],
        "summary": "List all files",
        "description": "List down all the spreadsheets can be accessed by the service account.",
        "operationId": "getAllFileList",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/Spreadsheet"
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Empty data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Something bad happened!"
                    },
                    "data": {
                      "type": "array",
                      "example": []
                    }
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "x-api-key": []
          }
        ]
      }
    },
    "/sheets/{spreadsheet_id}": {
      "get": {
        "tags": ["Sheet"],
        "summary": "Get the sheet's info",
        "description": "Get the sheets available by using spreadsheet id",
        "operationId": "getSheetsBySpreadsheetId",
        "parameters": [
          {
            "name": "spreadsheet_id",
            "in": "path",
            "description": "ID from the spreadsheet",
            "required": true,
            "schema": {
              "type": "string",
              "example": "1-Qi5_aizQiNTMRBuqboory9Ba7lyonxCjDCogASdVdg"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/SheetInfo"
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Empty data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Something bad happened!"
                    },
                    "data": {
                      "type": "array",
                      "example": []
                    }
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "x-api-key": []
          }
        ]
      }
    },
    "/sheets/{spreadsheet_id}/{sheet_name}": {
      "get": {
        "tags": ["Sheet"],
        "summary": "Get the sheet's data",
        "description": "Get the columns and the data with pagination.",
        "operationId": "getDataBySheetName",
        "parameters": [
          {
            "name": "spreadsheet_id",
            "in": "path",
            "description": "ID from the spreadsheet",
            "required": true,
            "schema": {
              "type": "string",
              "example": "1-Qi5_aizQiNTMRBuqboory9Ba7lyonxCjDCogASdVdg"
            }
          },
          {
            "name": "sheet_name",
            "in": "path",
            "description": "Title from the sheet",
            "required": true,
            "schema": {
              "type": "string",
              "example": "Sheet1"
            }
          },
          {
            "name": "offset",
            "in": "query",
            "description": "Pagination offset",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 2,
              "example": 2
            }
          },
          {
            "name": "per_page",
            "in": "query",
            "description": "Pagination page size",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 100,
              "example": 100
            }
          },
          {
            "name": "column_count",
            "in": "query",
            "description": "Maximum column number",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 10,
              "example": 10
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/SheetData"
                      }
                    },
                    "pagination": {
                      "$ref": "#/components/schemas/SheetPagination"
                    },
                    "columns": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/SheetColumn"
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Empty data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Something bad happened!"
                    },
                    "data": {
                      "type": "array",
                      "example": []
                    }
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "x-api-key": []
          }
        ]
      },
      "post": {
        "tags": ["Sheet"],
        "summary": "Append rows",
        "description": "Add new rows to the sheet.",
        "operationId": "appendSheetRow",
        "parameters": [
          {
            "name": "spreadsheet_id",
            "in": "path",
            "description": "ID from the spreadsheet",
            "required": true,
            "schema": {
              "type": "string",
              "example": "1-Qi5_aizQiNTMRBuqboory9Ba7lyonxCjDCogASdVdg"
            }
          },
          {
            "name": "sheet_name",
            "in": "path",
            "description": "Title from the sheet",
            "required": true,
            "schema": {
              "type": "string",
              "example": "Sheet1"
            }
          },
          {
            "name": "column_count",
            "in": "query",
            "description": "Maximum column number",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 10,
              "example": 10
            }
          },
          {
            "name": "input_options",
            "in": "query",
            "description": "Input options value",
            "required": false,
            "schema": {
              "type": "string",
              "default": "USER_ENTERED",
              "enum": ["USER_ENTERED", "RAW"]
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "data": {
                    "type": "array",
                    "example": [{ "name": "Jean", "email": "jean@appleseed.com" }, { "name": "Bunny", "email": "bunny@appleseed.com" }]
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/UpdateSheetResponse"
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Empty data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Something bad happened!"
                    },
                    "data": {
                      "type": "array",
                      "example": []
                    }
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "x-api-key": []
          }
        ]
      },
      "put": {
        "tags": ["Sheet"],
        "summary": "Update rows",
        "description": "Update certain rows inside sheet.",
        "operationId": "updateSheetRow",
        "parameters": [
          {
            "name": "spreadsheet_id",
            "in": "path",
            "description": "ID from the spreadsheet",
            "required": true,
            "schema": {
              "type": "string",
              "example": "1-Qi5_aizQiNTMRBuqboory9Ba7lyonxCjDCogASdVdg"
            }
          },
          {
            "name": "sheet_name",
            "in": "path",
            "description": "Title from the sheet",
            "required": true,
            "schema": {
              "type": "string",
              "example": "Sheet1"
            }
          },
          {
            "name": "column_count",
            "in": "query",
            "description": "Maximum column number",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 10,
              "example": 10
            }
          },
          {
            "name": "input_options",
            "in": "query",
            "description": "Input options value",
            "required": false,
            "schema": {
              "type": "string",
              "default": "USER_ENTERED",
              "enum": ["USER_ENTERED", "RAW"]
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "example": {
                  "4": { "email": "john@appleseed.com" },
                  "1": { "phone": "415-500-7000" }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/UpdateSheetResponse"
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Empty data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Something bad happened!"
                    },
                    "data": {
                      "type": "array",
                      "example": []
                    }
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "x-api-key": []
          }
        ]
      }
    },
    "/sheets/{spreadsheet_id}/{sheet_name}/{row}": {
      "get": {
        "tags": ["Sheet"],
        "summary": "Get the row data",
        "description": "Get data in certain row inside sheet.",
        "operationId": "getSheetDataByRows",
        "parameters": [
          {
            "name": "spreadsheet_id",
            "in": "path",
            "description": "ID from the spreadsheet",
            "required": true,
            "schema": {
              "type": "string",
              "example": "1-Qi5_aizQiNTMRBuqboory9Ba7lyonxCjDCogASdVdg"
            }
          },
          {
            "name": "sheet_name",
            "in": "path",
            "description": "Title from the sheet",
            "required": true,
            "schema": {
              "type": "string",
              "example": "Sheet1"
            }
          },
          {
            "name": "row",
            "in": "path",
            "description": "Row number",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32",
              "example": 2
            }
          },
          {
            "name": "column_count",
            "in": "query",
            "description": "Maximum column number",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 10,
              "example": 10
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "$ref": "#/components/schemas/SheetData"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Empty data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Something bad happened!"
                    },
                    "data": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "x-api-key": []
          }
        ]
      },
      "delete": {
        "tags": ["Sheet"],
        "summary": "Remove rows",
        "description": "Remove certain rows from sheet. Multiple rows using comma separator.",
        "operationId": "removeSheetRows",
        "parameters": [
          {
            "name": "spreadsheet_id",
            "in": "path",
            "description": "ID from the spreadsheet",
            "required": true,
            "schema": {
              "type": "string",
              "example": "1-Qi5_aizQiNTMRBuqboory9Ba7lyonxCjDCogASdVdg"
            }
          },
          {
            "name": "sheet_name",
            "in": "path",
            "description": "Title from the sheet",
            "required": true,
            "schema": {
              "type": "string",
              "example": "Sheet1"
            }
          },
          {
            "name": "row",
            "in": "path",
            "description": "Rows to be deleted. Use comma to remove multiple rows.",
            "required": true,
            "schema": {
              "type": "string",
              "example": "1,2,3"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "deleted_rows": {
                      "type": "integer",
                      "format": "int32",
                      "example": 3
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Empty data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Something bad happened!"
                    },
                    "deleted_rows": {
                      "type": "integer",
                      "format": "int32",
                      "example": 0
                    }
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "x-api-key": []
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "Spreadsheet": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "1-Qi5_aizQiNTMRBuqboory9Ba7lyonxCjDCogASdVdg"
          },
          "name": {
            "type": "string",
            "example": "sample-spreadsheet"
          },
          "modifiedTime": {
            "type": "string",
            "format": "date-time",
            "example": "2025-02-03T08:58:19.749Z"
          }
        }
      },
      "SheetInfo": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "example": "Sheet1"
          },
          "index": {
            "type": "integer",
            "format": "int32",
            "example": 0
          },
          "sheetId": {
            "type": "integer",
            "format": "int32",
            "example": 1
          },
          "rowCount": {
            "type": "integer",
            "format": "int32",
            "example": 1000
          },
          "columnCount": {
            "type": "integer",
            "format": "int32",
            "example": 10
          }
        }
      },
      "SheetData": {
        "type": "object",
        "example": {
          "_row": 2,
          "NO": 1,
          "SLUG": "test",
          "VIEWS": 100,
          "LIKE": 1000
        },
        "properties": {
          "_row": {
            "type": "integer",
            "format": "int32",
            "example": 0
          }
        }
      },
      "SheetPagination": {
        "type": "object",
        "properties": {
          "perPage": {
            "type": "integer",
            "format": "int32",
            "example": 20
          },
          "cellRange": {
            "type": "string",
            "example": "Sheet1!A2:J101"
          },
          "offset": {
            "type": "integer",
            "format": "int32",
            "example": 0
          },
          "nextOffset": {
            "type": "integer",
            "format": "int32",
            "example": 21
          },
          "totalItems": {
            "type": "integer",
            "format": "int32",
            "example": 100
          },
          "haveNext": {
            "type": "boolean"
          }
        }
      },
      "SheetColumn": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "example": "Title"
          },
          "cell": {
            "type": "string",
            "example": "Sheet1!A"
          }
        }
      },
      "UpdateSheetResponse": {
        "type": "object",
        "properties": {
          "spreadsheetId": {
            "type": "string",
            "example": "1-Qi5_aizQiNTMRBuqboory9Ba7lyonxCjDCogASdVdg"
          },
          "updatedCells": {
            "type": "integer",
            "format": "int32",
            "example": 1
          },
          "updatedColumns": {
            "type": "integer",
            "format": "int32",
            "example": 1
          },
          "updatedRows": {
            "type": "integer",
            "format": "int32",
            "example": 1
          },
          "updatedRange": {
            "type": "string",
            "example": "Sheet1!B4"
          }
        }
      }
    },
    "requestBodies": {},
    "securitySchemes": {
      "x-api-key": {
        "type": "apiKey",
        "name": "x-api-key",
        "in": "header"
      }
    }
  }
}
